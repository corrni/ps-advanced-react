{
  "verbose": true,
  "restartable": "rs",
  "ignore": [
    ".git",
    "log",
    "node_modules",
    "flow-typed",
    "**/__tests__/**/*",
    "lib/components"
  ],
  "execMap": {
    "js": "node_modules/.bin/babel-node"
  },
  "watch": [
    "lib/server.js",
    "lib/serverRender.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
